    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^5.4.15",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^1.6.1",
    "vue-tsc": "^1.8.27"
  }
